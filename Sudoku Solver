print('|_',board[7][0],'_|_',board[7][1],'_|_',board[7][2],'_| |_',board[7][3],'_|_',board[7][4],'_|_',board[7][5],'_| |_',board[7][6],'_|_',board[7][7],'_|_',board[7][8],'_|')
print('|_',board[8][0],'_|_',board[8][1],'_|_',board[8][2],'_| |_',board[8][3],'_|_',board[8][4],'_|_',board[8][5],'_| |_',board[8][6],'_|_',board[8][7],'_|_',board[8][8],'_|')
